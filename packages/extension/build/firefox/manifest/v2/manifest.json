{
	"name": "Gogogo",
	"version": "2025.4.8.1",
	"description": "Automate your browser with AI",
	"manifest_version": 2,
	"default_locale": "en",
	"icons": {
		"16": "assets/icons/icon_color_16x16.png",
		"32": "assets/icons/icon_color_32x32.png",
		"48": "assets/icons/icon_color_48x48.png",
		"128": "assets/icons/icon_color_128x128.png"
	},
	"browser_action": {
		"default_icon": "assets/icons/icon_color_16x16.png",
		"default_popup": "ui/action/index.html"
	},
	"options_ui": {
		"page": "ui/options/index.html",
		"open_in_tab": true
	},
	"sidebar_action": {
		"default_icon": {
			"16": "assets/icons/icon_color_16x16.png",
			"32": "assets/icons/icon_color_32x32.png"
		},
		"default_title": "Gogogo",
		"default_panel": "ui/sidebar/index.html"
	},
	"background": {
		"page": "assets/icons/bg-loader.html"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"match_about_blank": true,
			"js": [
				"content.js"
			],
			"run_at": "document_start",
			"all_frames": true
		}
	],
	"permissions": [
		"tabs",
		"debugger",
		"scripting",
		"cookies",
		"storage",
		"webNavigation",
		"desktopCapture",
		"nativeMessaging",
		"notifications"
	],
	"web_accessible_resources": [
		"resources/*.*",
		"assets/**/*.*",
		"ui/**/*.*"
	],
	"applications": {
		"gecko": {
			"id": "sagibrant@163.com",
			"strict_min_version": "58.0"
		}
	}
}